@{
    ViewData["Title"] = "Tasks";
    var user = (User)ViewBag.User;
}

<section>
    <div class="container">
        <div class="row">
            @foreach (var temp in ViewBag.Tasks)
            {
                <a href="@Url.Action("Task")/@temp.Id" class="col-3 mt-4">
                    <h5>@temp.Title</h5>
                    <p>@temp.Text</p>
                </a>
            }
        </div>
    </div>
</section>